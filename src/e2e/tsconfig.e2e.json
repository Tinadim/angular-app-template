{
    "compilerOptions": {
        "baseUrl": "./",
        "declaration": false,
        "downlevelIteration": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "lib": [
            "es2018",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "./dist",
        "paths": {

        },
        "removeComments": true,
        "sourceMap": true,
        "target": "es2017",
        "types": [
            "jest",
            "node",
            "puppeteer",
            "jest-environment-puppeteer",
            "expect-puppeteer",
            "global.d.ts"
        ]
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
