{
    "name": "client",
    "version": "0.0.0",
    "scripts": {
        "build": "ng build",
        "build:aot": "ng build --aot",
        "build:prod": "ng build --prod --aot",
        "build:ssr": "ng build --prod && ng run client:server:production",
        "dev:ssr": "ng run client:serve-ssr",
        "e2e": "ng e2e",
        "lint": "ng lint",
        "ng": "ng",
        "prerender": "ng run client:prerender",
        "serve:ssr": "node dist/client/server/main.js",
        "start": "ng serve",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.7",
        "@angular/common": "~9.1.7",
        "@angular/compiler": "~9.1.7",
        "@angular/core": "~9.1.7",
        "@angular/forms": "~9.1.7",
        "@angular/platform-browser": "~9.1.7",
        "@angular/platform-browser-dynamic": "~9.1.7",
        "@angular/platform-server": "~9.1.7",
        "@angular/router": "~9.1.7",
        "@nguniversal/express-engine": "^9.1.1",
        "akita": "^0.9.0",
        "express": "^4.15.2",
        "lodash": "^4.17.15",
        "rxjs": "~6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.6",
        "@angular/cli": "~9.1.6",
        "@angular/compiler-cli": "~9.1.7",
        "@ngneat/spectator": "^5.7.1",
        "@nguniversal/builders": "^9.1.1",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/jest": "^25.2.2",
        "@types/jest-when": "^2.7.1",
        "@types/lodash": "^4.14.151",
        "@types/lolex": "^5.1.0",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^26.0.1",
        "jest-preset-angular": "^8.2.0",
        "jest-when": "^2.7.1",
        "lolex": "^6.0.0",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    },
    "jest": {
        "collectCoverageFrom": [
        "src/**/*.js",
        "src/**/*.ts",
        "!src/environments/*.ts",
        "!src/tests/**/*.ts"
        ],
        "coverageDirectory": "coverage",
        "coveragePathIgnorePatterns": [
        "/node_modules/"
        ],
        "coverageReporters": [
        "lcov"
        ],
        "preset": "jest-preset-angular",
        "roots": [
        "src"
        ],
        "setupFilesAfterEnv": [
        "./src/jest-setup.ts"
        ]
    }
}
